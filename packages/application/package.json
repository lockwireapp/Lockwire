    "@react-native-firebase/perf": "18.7.3",
    "@react-navigation/native": "6.1.8",
    "@react-navigation/stack": "6.3.18",
    "@types/i18n-js": "3.8.6",
    "@lckw/lib-crypto": "~0.0.1-dev",
    "@lckw/lib-models": "~0.0.1-dev",
