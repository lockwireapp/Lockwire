{
  "name": "@lckw/application",
  "version": "0.0.1",
  "main": "src/index.ts",
  "scripts": {
    "dev": "expo start --dev-client",
    "lint": "prettier --write \"**/*.+(json|yml|css|md|gql)\" ./src && eslint --fix --ext .js,.jsx,.ts,.tsx ./src",
    "lint:ci": "prettier -c \"**/*.+(json|yml|css|md|gql)\" ./src && eslint --ext .js,.jsx,.ts,.tsx ./src",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "build:dev:android:local": "eas build --profile development --platform android --local",
    "build:dev:android:eas": "eas build --profile development --platform android",
    "postinstall": "sh scripts/postinstall.sh"
  },
  "dependencies": {
    "@expo/vector-icons": "13.0.0",
